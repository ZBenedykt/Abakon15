<src:WindowBaseClass
        x:Class="Abakon15WPF.Views.Windows.DocumentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:UC ="clr-namespace:Abakon15WPF.Views.Controls"
        xmlns:src="clr-namespace:Abakon15WPF.Views"
        xmlns:VM ="clr-namespace:Abakon15WPF.ViewModels"
        Title="{Resx ResxName=WisecomWPF.LangKeys.ResourceLang, Key=_documentsWindow_Window_Title}" 
        Icon="{Resx ResxName=WisecomWPF.LangKeys.ResourceLang, Key=WindowIcon}"
